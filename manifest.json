{
  "manifest_version": 3,
  "name": "LinkedIn Promoted Post Blocker",
  "version": "1.0",
  "description": "Blocks promoted posts on LinkedIn",
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["*://*.linkedin.com/*"],
      "js": ["content.js"],
      "css": ["styles.css"]
    }
  ]
} 